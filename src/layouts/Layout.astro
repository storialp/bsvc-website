---
interface Props {
  title: string;
}

const { title } = Astro.props;
import Navbar from "../components/Navbar";
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="Bocconi Students for Venture Capital" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <link rel="icon" href="/favicon.ico" sizes="any" />
    <title>{title}</title>
  </head>
  <body>
    <Navbar client:load path={Astro.url.pathname} />
    <slot />
  </body>
</html>
